{
  "default_attributes": {
    "ns3::SwitchNode::EcnEnabled": true,

    "ns3::QbbNetDevice::PauseTime": 5e-6,
    "ns3::QbbNetDevice::QbbEnabled": true,
    "ns3::QbbNetDevice::DynamicThreshold": true,

    "ns3::RdmaHw::ClampTargetRate": false,
    "ns3::RdmaHw::AlphaResumInterval": 1.0,
    "ns3::RdmaHw::RPTimer": 900.0,
    "ns3::RdmaHw::FastRecoveryTimes": 1,
    "ns3::RdmaHw::EwmaGain": 0.00390625,
    "ns3::RdmaHw::RateAI": "50Mb/s",
    "ns3::RdmaHw::RateHAI": "100Mb/s",
    "ns3::RdmaHw::L2BackToZero": false,
    "ns3::RdmaHw::L2ChunkSize": 4000,
    "ns3::RdmaHw::L2AckInterval": 4000,
    "ns3::RdmaHw::NackInterval": "500us",
    "ns3::RdmaHw::CcMode": 1,
    "ns3::SwitchNode::CcMode": 1,
    "ns3::RdmaHw::RateDecreaseInterval": 4,
    "ns3::RdmaHw::MinRate": "100Mb/s",
    "ns3::RdmaHw::Mtu": 1500,
    "ns3::RdmaHw::VarWin": true,
    "ns3::RdmaHw::FastReact": true,
    "ns3::RdmaHw::RateBound": true,

    "ns3::SwitchMmu::BufferSize": "12MiB"
  },

  "topology_file": "default-topology.json",
  "flows_file": "default-flows.json",

  "simulator_stop_time": 0,

  "ack_high_prio": false,
  "rng_seed": 50,

  "ecn": [
    {
      "bandwidth_bps": 25e9,
      "min_buf_bytes": 100e3,
      "max_buf_bytes": 400e3,
      "max_probability": 0.2
    },
    {
      "bandwidth_bps": 50e9,
      "min_buf_bytes": 200e3,
      "max_buf_bytes": 800e3,
      "max_probability": 0.2
    },
    {
      "bandwidth_bps": 100e9,
      "min_buf_bytes": 400e3,
      "max_buf_bytes": 1600e3,
      "max_probability": 0.2
    }
  ],

  "modules": [
    {
      "path": "ns3::SwitchBufferMonitor",
      "enable": false,
      "attributes": {
        "AvroOutputFile": "out_sw_mem.avro",
        "StartTime": 0,
        "StopTime": 0,
        "IntervalTime": 1e-6
      }
    },
    {
      "path": "ns3::QpMonitor",
      "enable": false,
      "attributes": {
        "AvroOutputFile": "out_qp.avro",
        "StartTime": 0,
        "StopTime": 1e9,
        "IntervalTime": 1e-6
      }
    },
    {
      "path": "ns3::RdmaModAnim",
      "enable": true,
      "attributes": {
        "XmlOutputFile": "out/anim.xml"
      }
    },
    {
      "path": "ns3::PfcMonitor",
      "enable": false,
      "attributes": {
        "AvroOutputFile": "out_pfc.avro"
      }
    },
    {
      "path": "ns3::TxMonitor",
      "enable": false,
      "attributes": {
        "AvroOutputFile": "out_link_bytes.avro"
      }
    }
  ]
}